i=0.05
d = i/(1+i)
V=1/(1+i)

p = d*V^(k)*((dane[22:93,2]/dane[21,2])+(dane[30:101,2]/dane[29,2])-((dane[22:93,2]/dane[21,2])*(dane[30:101,2]/dane[29,2])))
